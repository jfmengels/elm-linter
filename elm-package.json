{
    "version": "1.0.0",
    "summary": "A linter for Elm code",
    "repository": "https://github.com/jfmengels/elm-linter.git",
    "license": "BSD3",
    "source-directories": [
        "src",
        "test"
    ],
    "exposed-modules": [
        "Ast",
        "Ast.BinOp",
        "Ast.Expression",
        "Ast.Statement"
    ],
    "dependencies": {
        "elm-lang/core": "4.0.0 <= v < 5.0.0",
        "Bogdanp/elm-combine": "2.2.1 <= v < 3.0.0",
        "elm-community/list-extra": "3.1.0 <= v < 4.0.0"
    },
    "elm-version": "0.17.1 <= v < 0.18.0"
}
